<h1 style="text-align: center;">Quản Lý Chi Phí Chuyển Khoản</h1>
<hr>
<body>
    <form action="" method="post">
    
        <% inteRestates.forEach(function(inteRestate){ %>
           <div>
            <label for="">Kì hạn lãi suất</label>
            <input type="text"name="nameinterest" value="<%= inteRestate.Name %>">
    
            <label for="">Lãi suất</label>
            <input type="number" name="interest"  step=0.01 value="<%= inteRestate.interest %>">  
          
            <label for="">Số tháng</label>
            <input type="number" name="numberMonth" value="<%= inteRestate.numberMonth %>">  
           </div>
        <% }); %>
       
      
        <button style="margin-left:25vw;margin-right:11vw;" type="submit" id = "submit" class= "form-control acttc">Thay Đổi</button>
    </form>
</body>
   
