




<%- include ('../partials/templateheader.ejs') -%>
<%- include ('../partials/templateleft.ejs') -%>
<br />

<%- include ('../partials/templateright.ejs') -%>
<br />
<hr />

<body>

  <form action="" method="post">
      <input type="text" placeholder="Số Hóa Đơn | Số tài khoản   ">
      <input type="submit" value="Tìm Kiếm">
      </form>


  <div class="container">
    <h1 style="text-align: center;">Lịch Sử Nạp Tiền Của Khách Hàng</h1>
    <br>
  <table class="table table-striped table-dark">
      <thead>
        <tr>
          <th scope="col">Mã hóa đơn</th>
          <th scope="col">Số tài khoản</th>
          <th scope="col">Họ và tên</th>
          <th scope="col">Số tiền</th>
          <th scope="col">Ngày giao dịch</th>
          <th scope="col">Nội dung</th>
        </tr>
      </thead>
      <tbody>
          <% recharge.forEach(function(r){ %>
        <tr>
          <th scope="col"><%= r.code %></th>
          <th scope="col"><%= r.accuontRecharge %></th>
          <th scope="col"><%= r.nameRecharge %></th>
          <th scope="col"><%= r.Money %></th>
          <th scope="col"><%= r.date %></th>
          <th scope="col"><%= r.content %></th>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
  </div>
</body>
   
</div>

<%- include ('../partials/templatefooter.ejs') -%>